{
	"version": 3,
	"builds": [
	  {
		"src": "dist/server/entry.mjs",
		"use": "@vercel/node"
	  }
	],
	"routes": [
	  {
		"src": "/_astro/(.*)",
		"headers": {
		  "cache-control": "public, max-age=31536000, immutable"
		},
		"dest": "/_astro/$1"
	  },
	  {
		"src": "/_image",
		"dest": "/_render"
	  },
	  {
		"src": "/404",
		"dest": "/404.html"
	  },
	  {
		"src": "/simulador",
		"dest": "/simulador.html"
	  },
	  {
		"src": "/",
		"dest": "/index.html"
	  },
	  {
		"src": "/(.*)",
		"dest": "/404.html",
		"status": 404
	  }
	]
  }